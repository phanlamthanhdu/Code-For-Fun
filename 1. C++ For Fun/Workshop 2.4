#include <iostream>
using namespace std;

int main(){
    int x,y;
    cout << "Input : ";
    cout << "x = "; cin >> x;
    cout << "y = "; cin >> y;

    if(x!=0 || y!=0){
        int t = x;
        x = y;
        y = t;    
    }
    cout << "Output : x = " << x << " and y = " << y << endl;
    return 0;
}
