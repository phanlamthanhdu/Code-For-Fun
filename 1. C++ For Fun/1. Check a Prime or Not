#include <iostream>
using namespace std;
int main()
{
    int i,n,x;
    cin >> n;

    for (i=1; i <= n; i++)
    {   
        if (n % i == 0)
        {
            x +=i;
        }
    } 
    if (x == ++n){
        cout << --n << " is a prime";
    }
    else { 
        cout << --n << " is not a prime"; 
    }

    return 0;
}
